{
	"Resources" : {
	    "MyDB": {
	        "Type": "AWS::RDS::DBInstance",
	        "Properties": {
	            "AllocatedStorage": "100",
	            "DBInstanceClass": "db.t3.small",
	            "CopyTagsToSnapshot": true,
	            "BackupRetentionPeriod": 4,
	            "Engine": "MySQL",
	            "EngineVersion": "5.7.22",
	            "Iops": "1000",
	            "MasterUsername": {
	                "Ref": "DBUser"
	            },
	            "MasterUserPassword": {
	                "Ref": "DBPassword"
	            },
	            "StorageEncrypted":true,
	            "MultiAZ": true
	        }
	    },
	    "MyDB1": {
	        "Type": "AWS::RDS::DBInstance",
	        "Properties": {
	            "AllocatedStorage": "100",
	            "DBInstanceClass": "db.t3.small",
	            "Engine": "MySQL",
	            "EngineVersion": "5.7.22",
	            "Iops": "1000",
	            "StorageEncrypted":true,
	            "MasterUsername": {
	                "Ref": "DBUser"
	            },
	            "MasterUserPassword": {
	                "Ref": "DBPassword"
	            }
	        }
	    }
	}
}
