{
  "locals": [
    {
      "test_description": "no google_container_cluster resource"
    }
  ],
  "provider": [
    {
      "google": [
        {
          "credentials": "${file(\"creds.json\")}",
          "project": "redlock-dev-playgroud"
        }
      ]
    },
    {
      "google-beta": [
        {
          "credentials": "${file(\"creds.json\")}",
          "project": "redlock-dev-playgroud"
        }
      ]
    }
  ],
  "resource": [
    {
      "not_google_container_cluster": [
        {
          "prb-tft-kube": [
            {
              "network_policy": [
                {
                  "enabled": true,
                  "provider": "CALICO"
                }
              ],
              "addons_config": [
                {
                  "http_load_balancing": [
                    {
                      "disabled": true
                    }
                  ],
                  "istio_config": [
                    {
                      "auth": "AUTH_MUTUAL_TLS",
                      "disabled": false
                    }
                  ],
                  "kubernetes_dashboard": [
                    {
                      "disabled": true
                    }
                  ],
                  "network_policy_config": [
                    {
                      "disabled": true
                    }
                  ]
                }
              ],
              "database_encryption": [
                {
                  "key_name": "projects/redlock-dev-playgroud/locations/us-central1/keyRings/prb-keyring/cryptoKeys/prb-key-kub",
                  "state": "ENCRYPTED"
                }
              ],
              "enable_binary_authorization": false,
              "enable_kubernetes_alpha": false,
              "enable_legacy_abac": false,
              "initial_node_count": 1,
              "ip_allocation_policy": [
                {
                  "use_ip_aliases": true
                }
              ],
              "location": "us-central1-a",
              "logging_service": "logging.googleapis.com",
              "master_auth": [
                {
                  "client_certificate_config": [
                    {
                      "issue_client_certificate": true
                    }
                  ],
                  "password": "",
                  "username": ""
                }
              ],
              "monitoring_service": "monitoring.googleapis.com",
              "name": "prb-tft-kube",
              "network": "projects/redlock-dev-playgroud/global/networks/default",
              "node_config": [
                {
                  "image_type": "COS",
                  "oauth_scopes": [
                    "https://www.googleapis.com/auth/logging.write",
                    "https://www.googleapis.com/auth/monitoring"
                  ]
                }
              ],
              "pod_security_policy_config": [
                {
                  "enabled": false
                }
              ],
              "private_cluster_config": [
                {
                  "enable_private_endpoint": false,
                  "enable_private_nodes": true,
                  "master_ipv4_cidr_block": "172.16.0.0/28"
                }
              ],
              "provider": "google-beta",
              "subnetwork": "projects/redlock-dev-playgroud/regions/us-central1/subnetworks/default"
            }
          ],
