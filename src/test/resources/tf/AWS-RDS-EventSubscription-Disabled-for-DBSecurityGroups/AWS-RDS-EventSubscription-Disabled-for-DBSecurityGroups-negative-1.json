{
  "locals": [
    {
      "test_description": "there is aws_db_instance but has exists subscription with source_type db-security-group and no enabled"
    }
  ],
  "resource": [
    {
      "aws_db_instance": [
        {
          "default": [
            {
              "allocated_storage": 10,
              "db_subnet_group_name": "my_database_subnet_group",
              "engine": "mysql",
              "engine_version": "5.6.17",
              "instance_class": "db.t2.micro",
              "name": "mydb",
              "parameter_group_name": "default.mysql5.6",
              "password": "bar",
              "username": "foo"
            }
          ]
        }
      ]
    },

    {
      "aws_db_event_subscription": [
        {
          "default": [
            {
              "event_categories": [
                "availability",
                "deletion"
              ],
              "name": "event-sub1",
              "enabled": false,
              "sns_topic": "${aws_sns_topic.default.arn}",
              "source_ids": [
                "${aws_db_instance.default.id}"
              ],
              "source_type": "db-security-group"
            }
          ]
        }
      ]
    },
    {
      "aws_db_event_subscription": [
        {
          "subs2": [
            {
              "event_categories": [
                "availability",
                "deletion"
              ],
              "name": "event-sub2",
              "sns_topic": "${aws_sns_topic.default.arn}",
              "source_ids": [
                "${aws_db_instance.default.id}"
              ],
              "source_type": "db-security-group"
            }
          ]
        }
      ]
    }
  ]
}